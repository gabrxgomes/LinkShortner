{
  "version": 2,
  "builds": [
    {
      "src": "pom.xml",
      "use": "@vercel/java"
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "/LinkShortenerApplication"
    },
    {
      "src": "/[^/]+",
      "dest": "/LinkShortenerApplication"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "SPRING_PROFILES_ACTIVE": "production"
  }
}
